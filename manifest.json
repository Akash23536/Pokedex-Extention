{
    "manifest_version": 3,
    "name": "Pokédex",
    "version": "1.0",
    "description": "Search for Pokémon and display random Pokémon on refresh.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": ["activeTab"]
  }
  